{
  "name": "my-thrifting-buddy-backend",
  "version": "1.0.0",
  "description": "Backend API for My Thrifting Buddy App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:help": "echo \"Usage: npm run test <image-path>\" && echo \"Example: npm run test ./test-images/shirt.jpg\"",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "keywords": [
    "thrifting",
    "buddy",
    "ai",
    "openai",
    "resale",
    "secondhand"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.91.0",
    "aws-sdk": "^2.1513.0",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pg": "^8.16.3",
    "sharp": "^0.33.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
